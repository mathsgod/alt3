<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="robots" content="noindex, nofollow">
    <base href="{{base}}" />

    <title>{{app.config.user.company}} | {{module.name}}</title>

    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="{{composer_base}}/vendor/fortawesome/font-awesome/css/all.min.css">

    <!-- DataTables -->
    <link rel="stylesheet" href="{{system_base}}/plugins/datatables-bs4/css/dataTables.bootstrap4.css">

    <!-- Theme style -->
    <link rel="stylesheet" href="{{system_base}}/adminlte/dist/css/adminlte.min.css">

    <!-- Google Font: Source Sans Pro -->
    <!-- link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet"-->

    <link rel="stylesheet" href="{{system_base}}/css/main.css">

    <!-- OverlayScrollbars -->
    <link rel="stylesheet" href="{{system_base}}/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">

    <!-- fullCalendar -->
    <link rel="stylesheet" href="{{system_base}}/plugins/fullcalendar/main.min.css">
    <link rel="stylesheet" href="{{system_base}}/plugins/fullcalendar-interaction/main.min.css">
    <link rel="stylesheet" href="{{system_base}}/plugins/fullcalendar-daygrid/main.min.css">
    <link rel="stylesheet" href="{{system_base}}/plugins/fullcalendar-timegrid/main.min.css">
    <link rel="stylesheet" href="{{system_base}}/plugins/fullcalendar-bootstrap/main.min.css">

    <!-- fancybox -->
    <link rel="stylesheet" href="{{system_base}}/plugins/fancybox/jquery.fancybox.min.css">

    <!-- SweetAlert2 -->
    <link rel="stylesheet" href="{{system_base}}/plugins/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css">

    <!-- Regenerator -->
    <script src="{{system_base}}/plugins/regenerator-runtime/runtime.min.js"></script>

    <!-- VUE-->
    {% if app.config.user.development %}
    <script src="{{system_base}}/plugins/vue/vue.js"></script>
    {% else %}
    <script src="{{system_base}}/plugins/vue/vue.min.js"></script>
    {% endif %}
    <script src="{{system_base}}/plugins/vue/vue-resource.min.js"></script>

    <link rel="stylesheet" href="{{system_base}}/alt-component/dist/alt-component.css">
    <script src="{{system_base}}/alt-component/dist/alt-component.umd.min.js"></script>

    <!-- REQUIRED SCRIPTS -->

    <!-- jQuery -->
    <script src="{{system_base}}/plugins/jquery/jquery.min.js"></script>

    <!-- jQuery UI 1.11.4 -->
    <script src="{{system_base}}/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>

    <!-- Bootstrap 4 -->
    <script src="{{system_base}}/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- DataTables -->
    <script src="{{system_base}}/plugins/datatables/jquery.dataTables.js"></script>
    <script src="{{system_base}}/plugins/datatables-bs4/js/dataTables.bootstrap4.js"></script>

    <!-- Select2 -->
    <link rel="stylesheet" href="{{system_base}}/plugins/select2/css/select2.min.css">
    <link rel="stylesheet" href="{{system_base}}/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
    <script src="{{system_base}}/plugins/select2/js/select2.full.min.js"></script>

    <!-- bootstrap-datepicker -->
    <link rel="stylesheet" href="{{system_base}}/plugins/bootstrap-datepicker/css/bootstrap-datepicker.min.css">
    <script src="{{system_base}}/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>

    <!-- iCheck -->
    <link rel="stylesheet" href="{{system_base}}/plugins/icheck-bootstrap/icheck-bootstrap.min.css">

    <!-- jquery validation-->
    <script src="{{system_base}}/plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="{{system_base}}/plugins/jquery-validation/additional-methods.min.js"></script>

    <script src="{{system_base}}/plugins/moment/moment-with-locales.min.js"></script>

    <!-- Fullcalendar -->
    <script src="{{system_base}}/plugins/fullcalendar/main.min.js"></script>
    <script src="{{system_base}}/plugins/fullcalendar-daygrid/main.min.js"></script>
    <script src="{{system_base}}/plugins/fullcalendar-timegrid/main.min.js"></script>
    <script src="{{system_base}}/plugins/fullcalendar-interaction/main.min.js"></script>
    <script src="{{system_base}}/plugins/fullcalendar-bootstrap/main.min.js"></script>

    <!-- bootstrap select -->
    <link rel="stylesheet" href="{{system_base}}/plugins/bootstrap-select/css/bootstrap-select.min.css" />
    <script type="text/javascript" src="{{system_base}}/plugins/bootstrap-select/js/bootstrap-select.min.js" defer></script>

    <!-- bootstrap-datetimepicker -->
    <link rel="stylesheet" href="{{system_base}}/plugins/bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css" />
    <script type="text/javascript" src="{{system_base}}/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js" defer></script>

    <!-- daterangepicker.com -->
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />

    <!-- tempusdominus-bootstrap4 -->
    <link rel="stylesheet" href="{{system_base}}/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css" />
    <script type="text/javascript" src="{{system_base}}/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js" defer></script>

    <!-- plugins -->
    {% for plugin in plugins %}
    {% for js in plugin.setting.js %}
    <script src="{{plugin.base}}/{{js}}" defer></script>
    {% endfor %}

    {% for css in plugin.setting.css %}
    <link rel="stylesheet" type="text/css" href="{{plugin.base}}/{{css}}" />
    {% endfor %}


    {% endfor %}


    <!-- overlayScrollbars -->
    <script src="{{system_base}}/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js" defer></script>

    <!-- AdminLTE App -->
    <script src="{{system_base}}/adminlte/dist/js/adminlte.js" defer></script>

    <!-- Bootbox -->
    <script src="{{system_base}}/plugins/bootbox/bootbox.all.min.js" defer></script>

    <script src="{{system_base}}/plugins/fancybox/jquery.fancybox.min.js" defer></script>

    <!-- webpack -->
    <script src="{{system_base}}/dist/api.js" defer></script>
    <script src="{{system_base}}/dist/main.js" defer></script>

    <!-- SweetAlert2 -->
    <script src="{{system_base}}/plugins/sweetalert2/sweetalert2.min.js" defer></script>


    <!-- Custom js -->
    {% for js in custom_js %}
    <script src="{{js}}" defer></script>
    {% endfor %}



    <script src="{{system_base}}/src/jquery.ajaxbox.js" defer></script>
</head>

