            {{custom_header|raw}}

            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0 text-dark">{{header.title|trans}}</h1>
                        </div><!-- /.col -->
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">{{'Home'|trans}}</a></li>
                                {% if module %}
                                <li class="breadcrumb-item active"><a href="{{module.name}}">{{module.name|trans}}</a></li>
                                {% endif %}

                                <li class="breadcrumb-item active"><a href="#">{{header.title}}</a></li>

                            </ol>
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->

            <!-- Main content -->
            <div class="content">
                <div class="container-fluid">
                    {% for alert in alerts %}
                    <div class="alert alert-{{alert.type}} alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                        <h5 class="
                        {% if not alert.description %}
                            mb-0
                        {% endif %}"><i class="{{alert.icon}}"></i> {{alert.message}}</h5>
                        {{alert.description|raw}}
                    </div>
                    {% endfor %}

                    {% for callout in callouts %}
                    <div class="callout callout-{{callout.type}}">
                        <h5>{{callout.title}}</h5>
                        {% if callout.description %}
                        <p>{{callout.description|raw}}</p>
                        {% endif %}
                    </div>
                    {% endfor %}

                    {{navbar|raw}}

                    {{content|raw}}
                </div><!-- /.container-fluid -->
