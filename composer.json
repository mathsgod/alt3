{
    "name": "mathsgod/alt3",
    "description": "Raymond framework 3",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Raymond Chong",
            "email": "mathsgod@yahoo.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../hostlink-fileman",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "composer",
            "url": "https://raymond2.hostlink.com.hk/satis/web"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "class/"
        },
        "files": [
            "function/default.php"
        ]
    },
    "require": {
        "php": ">=7.0",
        "mathsgod/r": "^3.0",
        "mathsgod/r-db": "^3.1",
        "twig/twig": "^2.12",
        "firebase/php-jwt": "^5.0",
        "twig/extensions": "^1.5",
        "fortawesome/font-awesome": "^5.11",
        "mathsgod/alt-app-core": "^1.1",
        "symfony/yaml": "^3.4",
        "mathsgod/html": "^1.0",
        "mathsgod/p-query": "^2.2",
        "mathsgod/r-graphql": "^1.3",
        "mathsgod/hl-holiday": "^1.2",
        "mathsgod/hl-weather": "^2.0",
        "mathsgod/r-translate": "^1.0",
        "mathsgod/gql-php-generator": "^1.0",
        "mathsgod/r-webauthn": "^1.0",
        "mathsgod/date": "^1.0",
        "mathsgod/hostlink-fileman": "^2.0"
    },
    "require-dev": {
        "box/spout": "^2.7",
        "phpunit/phpunit": "^6.5",
        "vakata/jstree": "^3.3",
        "sepia/po-parser": "^5.1",
        "ckeditor/ckeditor": "^4.13",
        "corejavascript/typeahead.js": "^1.3",
        "kevinchappell/form-builder": "^3.4"
    },
    "suggest": {
        "phpoffice/phpexcel": "phpoffice",
        "phpunit/phpunit": "phpunit",
        "monolog/monolog": "Allows more advanced logging of the application flow",
        "corejavascript/typeahead.js": "fast and fully-featured autocomplete library"
    }
}
